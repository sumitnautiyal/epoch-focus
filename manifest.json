{
  "manifest_version": 2,
  "name": "Epoch Focus",
  "version": "0.1.0",
  "description": "A zero-latency new tab page featuring Stoic wisdom. Part of the Epoch Suite.",
  "author": "Sumit Nautiyal",
  "icons": {
    "48": "src/icons/icon-48.png",
    "96": "src/icons/icon-96.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "epoch-focus@thecsjourney.com",
      "strict_min_version": "115.0"
    }
  },
  "permissions": [
    "storage"
  ]
}